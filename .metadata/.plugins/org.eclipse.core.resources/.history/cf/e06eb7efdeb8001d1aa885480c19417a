package modal;

import java.util.Random;

public class Sall_attont implements Runnable {
	
	int pation ;
	
	
	public int arive_lasall(int pation) {
		pation ++;
		return pation;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		while (true) {
			pation++;
			arive_lasall(pation);
			
		}
		
		
	}
	

}
